{
  "name": "gatsby-starter-emulsify-drupal",
  "version": "1.0.0-alpha.2",
  "description": "Emulsify Gatsby Starter using Emulsify Gatsby Theme for Drupal projects",
  "main": "index.js",
  "keywords": [
    "gatsby"
  ],
  "author": "Evan Willhite <evan@fourkitchens.com>",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.5.4",
    "@babel/preset-env": "^7.5.4",
    "@storybook/addon-actions": "^5.1.9",
    "@storybook/addon-links": "^5.1.9",
    "@storybook/addons": "^5.1.9",
    "@storybook/react": "^5.1.9",
    "autoprefixer": "^9.6.1",
    "babel-preset-minify": "^0.5.0",
    "cssnano": "^4.1.10",
    "mini-css-extract-plugin": "^0.7.0",
    "postcss-import": "^12.0.1",
    "postcss-import-ext-glob": "^1.1.0",
    "postcss-loader": "^3.0.0",
    "postcss-nested": "^4.1.2",
    "postcss-simple-vars": "^5.0.2",
    "stylelint": "^10.1.0",
    "stylelint-config-standard": "^18.3.0",
    "stylelint-webpack-plugin": "^0.10.5",
    "webpack": "^4.35.3",
    "webpack-cli": "^3.3.6"
  },
  "dependencies": {
    "@babel/cli": "^7.6.4",
    "add-attributes-twig-extension": "^0.1.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-drupal-behaviors": "^0.1.1",
    "bem-twig-extension": "^0.1.1",
    "clean-webpack-plugin": "^3.0.0",
    "concurrently": "^5.0.0",
    "cross-env": "5.0.5",
    "css-loader": "^3.2.0",
    "gatsby": "^2.13.15",
    "gatsby-theme-emulsify": "^1.0.0-alpha.5",
    "glob": "^7.1.4",
    "imagemin-webpack-plugin": "^2.4.2",
    "mini-css-extract-plugin": "^0.7.0",
    "postcss-custom-media": "^7.0.8",
    "postcss-mixins": "^6.2.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "svg-sprite-loader": "^4.1.6",
    "twig": "^1.13.3",
    "twig-drupal-filters": "^2.0.0",
    "twig-loader": "https://github.com/AmazeeLabs/twig-loader"
  },
  "scripts": {
    "styleguide": "gatsby develop",
    "build-styleguide": "gatsby build",
    "deploy-styleguide": "gatsby build --prefix-paths && gh-pages -d public",
    "storybook": "start-storybook -s ./dist -p 6006",
    "build-storybook": "build-storybook",
    "babel": "npx babel components -w -d dist/js --ignore 'components/**/*.component.js','components/**/*.stories.js' --presets=@babel/preset-env,babel-preset-minify --plugins=babel-plugin-drupal-behaviors",
    "build-babel": "npx babel components -d dist/js --ignore 'components/**/*.component.js','components/**/*.stories.js' --presets=@babel/preset-env,babel-preset-minify --plugins=babel-plugin-drupal-behaviors",
    "webpack": "webpack -d --watch --config ./webpack/webpack.config.js --display-error-details",
    "build-webpack": "webpack -p --config ./webpack/webpack.config.js --display-error-details",
    "postinstall": "yarn build-webpack",
    "develop": "concurrently --raw \"yarn webpack\" \"yarn babel\" \"yarn storybook\""
  }
}
