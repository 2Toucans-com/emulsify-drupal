{% embed "@templates/_default.twig" with {
  menu_items: header.menu_items,
  footer_menu_items: footer.menu_items,
  page: {
    hero: TRUE,
    pre_content: TRUE
  },
} %}

  {% block hero %}
    {% include "@molecules/page-hero/_page-hero.twig" %}
  {% endblock %}

  {% block page_content %}
    {% embed "@base/layouts/_wrap.twig" with {
      wrap_modifiers: text_modifiers,
      wrap_inner_modifiers: text_inner_modifiers,
      wrap_extra_classes: text_extra_classes,
    } %}
      {% block wrap_content %}
        {% include "@atoms/02-text/text/paragraphs/_paragraph.twig" with {
          paragraph_modifiers: paragraph1_modifiers,
          paragraph_content: paragraph1,
        } %}
        {% include "@atoms/02-text/text/paragraphs/_paragraph.twig" with {
          paragraph_content: paragraph2,
        } %}
      {% endblock %}
    {% endembed %}

    {% include "@molecules/text-with-media/text-with-media.twig" %}

    {% include "@molecules/quote/quote.twig" %}

    {% include "@molecules/cta/cta.twig" %}

    {% include "@molecules/text-columns/text-columns.twig" %}

    {% include "@organisms/card-grid/card-grid.twig" %}

    {% include "@molecules/quote/quote.twig" with {
      quote_image: quote_2_image,
    } %}

    {% include "@molecules/text-with-media/text-with-media.twig" with {
      text_with_media_direction: text_with_media_2_direction
    } %}

    {% embed "@base/layouts/_wrap.twig" with {
      wrap_modifiers: ['contained']
    } %}
      {% block wrap_content %}
        {% include "@atoms/04-images/00-image/figure.twig" %}
      {% endblock %}
    {% endembed %}

    {% include "@molecules/cta/cta.twig" with {
      cta_modifiers: cta_2_modifiers,
      cta_bg_color: cta_2_bg_color,
      cta_title: cta_2_title,
      cta_text: cta_2_text,
      cta_link_text: cta_2_link_text,
      cta_link_url: cta_2_link_url,
    } %}

    {% include "@organisms/teaser-grid/teaser-grid.twig" %}
    
    {% include "@molecules/gallery/gallery.twig" %}

    {% embed "@molecules/guide-table/guide-table.twig" %}
      {% block guide_table_items %}
        {% for item in guide_table_items %}
          {% include "@molecules/guide-table/_guide-table-item.twig" with {
            guide_table_item_legend: item.guide_table_item_legend,
            guide_table_item_definition: item.guide_table_item_definition,
          } %}
        {% endfor %}
      {% endblock %}
    {% endembed %}

    {% include "@organisms/teaser-grid/teaser-grid.twig" with {
      teaser_grid_label: teaser_grid_2_label,
      teaser_grid_blockname: teaser_grid_2_blockname,
      grid_inner_modifiers: grid_inner_2_modifiers,
      teasers: teasers_2,
    } %}

    {% include "@organisms/card-grid/card-grid.twig" with {
      card_grid_modifiers: ['gray'],
      cards: cards2,
    } %}

    {% include "@molecules/video-content/video-content.twig" %}

    {% include "@molecules/cta/cta.twig" with {
      cta_bg_color: cta_3_bg_color,
      cta_title: cta_3_title,
      cta_text: cta_3_text,
      cta_link_text: cta_3_link_text,
      cta_link_url: cta_3_link_url,
    } %}

  {% endblock %}

{% endembed %}
