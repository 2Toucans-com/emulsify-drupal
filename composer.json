    "type": "drupal-theme",
    "license": "MIT",
    "authors": [
        {
            "name": "Evan Willhite",
            "email": "evan@fourkitchens.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
